<link rel="stylesheet" href="/css/lists.css">
<script src="/scripts/entries.js" defer></script>
<link rel="stylesheet" href="/css/entries.css">
<link rel="stylesheet" href="/css/redirectionLinkEntry.css">
<script src="/scripts/redirectionLinkEntry.js" defer></script>



<section class="list_page">

    <div class="list_titleContainer">

        {{>redirectionLinkEntry
                routeName="homeMenu"
                headerMode=true
                title="Home Menu Items"
                headerClass="list_title"
                defaultIconRoute="/img/icon-info.svg"
        }}

    </div>


    <div class="list_scrollableContent">

        {{#each data}}
            {{>redirectionLinkEntry
                    routeName="homeMenu"
                    defaultIconRoute="/img/icon-info.svg"
                    deleteRoute=(strConcat "/homeMenu/delete/" id )
                    reorderable="true"
                    reorderable_list = ../data
                    reorderable_Route="/homeMenu/swap"
            }}
        {{/each}}
    </div>
</section>


